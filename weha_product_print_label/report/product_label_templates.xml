<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Simple Label Template for 2x7 (Table Cell Format - Without Logo) -->
    <template id="report_simple_label_2x7">
        <t t-set="table_style" t-value="'width:97mm;height:36mm;' + table_style"/>
        <td t-att-style="make_invisible and 'visibility:hidden;'">
            <div class="o_label_full" t-att-style="table_style" style="text-align: center; padding: 2mm; display: flex; flex-direction: column; justify-content: center; align-items: center; height: 100%;">
                <div class="o_label_name" style="font-size: 11pt; font-weight: normal; margin-bottom: 2mm; margin-top: 1mm; text-align: center;">
                    <span t-field="product.display_name"/>
                </div>
                <div style="font-size: 24pt; font-weight: bold; margin-bottom: 2mm; text-align: center; line-height: 1.2;">
                    <strong t-out="pricelist._get_product_price(product, 1, currency=pricelist.currency_id or product.currency_id)"
                            t-options="{'widget': 'monetary', 'display_currency': pricelist.currency_id or product.currency_id}"/>
                </div>
                <t t-if="barcode">
                    <t t-set="barcode_img" t-value="generate_barcode(barcode, width=400, height=60)"/>
                    <div t-if="barcode_img" style="text-align: center; margin: 1mm 0; width: 100%;">
                        <img t-att-src="barcode_img" style="width: 95%; max-width: 95%; height: 10mm;" alt="Barcode"/>
                    </div>
                    <div style="font-size: 7pt; text-align: center; font-family: monospace; margin-top: 0.5mm;">
                        <span t-out="barcode"/>
                    </div>
                </t>
            </div>
        </td>
    </template>

